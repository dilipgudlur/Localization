find . -name Localization.jar | xargs rm
rm -rf out/
mkdir out
javac -sourcepath src/ -d out/ src/edu/cmu/pandaa/framework/App.java -classpath lib/mdsj.jar 
javac -sourcepath src/ -d out/ src/edu/cmu/pandaa/module/ConstructGeometryModule.java -classpath lib/mdsj.jar 
javac -sourcepath src/ -d out/ src/edu/cmu/pandaa/module/GeometryMatrixModule.java -classpath lib/mdsj.jar 
javac -sourcepath src/ -d out/ src/edu/cmu/pandaa/stream/RawAudioFileStream.java
jar cf Localization.jar -C out/ .

